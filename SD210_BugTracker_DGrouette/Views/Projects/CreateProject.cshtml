@model ProjectManipulationViewModel
@{
    ViewBag.Title = "Project Creation";
}

<h4>Create a new project: </h4>

<form method="post" enctype="multipart/form-data" class="form-vertical">
    <div class="form-group">
        @Html.ValidationMessageFor(p => p.Title)
    </div>
    <div class="form-group">
        @Html.LabelFor(p => p.Title, "Project Title: ")
        @Html.TextBoxFor(p => p.Title, new { placeholder = "New Project", @class = "form-control" })
    </div>
    <button type="submit" class="btn btn-default">Submit</button>
</form>

