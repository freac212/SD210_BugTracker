@model ProjectManipulationViewModel
@{
    ViewBag.Title = "Project Creation";
}

<h4>Create a new project: </h4>

<form method="post" enctype="multipart/form-data">
    <div class="form-group">
        @Html.ValidationMessageFor(p => p.Title)
    </div>
    <div class="form-group">
        @Html.LabelFor(p => p.Title, "Project Title: ", new { @class="form-control"})
    </div>
    <div class="form-group">

        @Html.TextBoxFor(p => p.Title, new { placeholder = "New Project", @class="form-control" })
    </div>
    <div class="form-group">
        <button type="submit" class="form-control btn-default">Submit</button>
    </div>
</form>

